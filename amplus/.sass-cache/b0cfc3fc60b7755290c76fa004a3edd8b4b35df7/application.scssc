#main-menu {
    a {
    	border: none;
    	text-align: left;
    }
    > ul > li > a {
        line-height: 50px;
        padding: 0 15px;
        @media only screen and (min-width: 828px) and (max-width: 1020px) {
            padding: 0 8px;
        }
    }
    a.downarrow:after, a.rightarrow:after {
		// content: ' Â»';
		content: '\f101';
		font-family: fontAwesome;
		margin-left: 10px;
		font-size: 110%;
	}
	> ul > li > ul {
		a, a:visited {
			padding: 10px 15px;
			color: #444;
			background: #eee;
			border-bottom: 1px solid #ddd;
		}
		a:hover, a.selected {
			background: #e0e0e0;
			color: red;
		}
	}
	> ul {
		@media only screen and (max-width: 828px) {
			display: none;
		}
	}
	> ul > li > a:hover, > ul > li > a.selected {
		background: rgba(255,255,255,.2);
	}
}
.ddsmoothmenu ul li ul li a {
    width: 180px;
}


.language_switcher {
    position: absolute;
    z-index: 2;
    right: 60px;
    top: 7px;
    @media only screen and (min-width: 828px) and (max-width: 1020px) {
        right: 40px;
    }
}


// Buttons
a.button, a.button:visited, a.button:hover {
	background: #888;
	color: white;
	text-shadow: none;
	border: none;
	padding: 8px 15px;
	font-size: 90%;
	line-height: 25px;
	&:hover {
		background: #555 !important;
	}
	i {
		color: white;
		float: left;
		margin-right: 15px;
	}
	&.small {
		font-size: 80%;
		padding: 7px 13px;
		line-height: 22px;
	}
	&.large {
		font-size: 110%;
		padding: 9px 15px;
		line-height: 30px;
	}
}

.widget-container {
    margin-bottom: 30px;
}

footer {
    .social {
        position: absolute;
        right: 20px;
        top: 15px;
        .fc-webicon {
            &, &:hover {
                margin-bottom: 0 !important;
                margin-top: 0 !important;
            }
            display: block;
            @include opacity(.7);
            &:hover {
                @include opacity(1);
            }
        }
        .bfi_social {
            float: right;
            margin-left: 10px;
        }
    }
    small {
        font-style: italic;
    }
}


// fancybox
a.fancybox, a.lightbox {
	margin-bottom: 20px;
	display: block;
	position: relative;
	background: black;
	color: white;
	z-index: 1;
	&:before {
		z-index: 2;
		position: absolute;
		//margin-top: 50%;
		//top: -21.5px;
		bottom: 0;
		left: 0;
		right: 0;
		background-position: center center;
		line-height: 100%;
		text-align: center;
		color: white;
		visibility: hidden;
		
		
        top: auto;
        margin-top: 0;
        height: 58%;
        @media only screen and (max-width: 960px) and (min-width: 767px) {
            height: 61%;
        }
        @media only screen and (max-width: 767px) {
            height: 56%;
        }
	}
	&.hovered:before {
	    visibility: visible;
	}
	img {
		position: relative;
		z-index: 3;
		&:hover {
			@include opacity(.4);
		}
	}
}
body.has-sidebar-left, body.has-sidebar-right {
    a.fancybox:before, a.lightbox:before {
        height: 61%;
        @media only screen and (max-width: 960px) and (min-width: 767px) {
            height: 64%;
        }
        @media only screen and (max-width: 767px) {
            height: 56%;
        }
    }
}

// fancybox skin
.fancybox-skin {
	padding: 0 !important;
	border-radius: 0 !important;
	.fancybox-title {
		width: 100%;
		margin: 0;
		span {
			border-radius: 0;
			background: rgba(0,0,0,.5);
			position: absolute;
			padding: 4px 0;
			width: 100%;
		}
	}
}

.nivo-caption {
    left: auto;
    right: auto;
    top: 0;
    bottom: auto !important;
    height: auto;
    width: auto !important;
    background: none !important;
    &.left {
        left: 30px;
    }
    &.right {
        text-align: right;
        right: 30px;
    }
}

.portfolio.column {
    .lightbox, .fancybox {
        margin-bottom: 10px;
    }
    h4 {
        margin-bottom: 20px;
    }
}


a.csh {
    color: black;
    position: absolute;
    @include transition-property(none);
    @include transition-duration(0);
    cufon {
        @include opacity(0);
    }
    &:before, &:after {
        visibility: hidden !important;
    }
