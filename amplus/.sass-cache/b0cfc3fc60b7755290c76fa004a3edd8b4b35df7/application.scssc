// NOTES:
// min/max width 767 => 828

@import "compass";
@import "compass/css3/transition";
@import "compass/css3/box-shadow";
@import "compass/css3/opacity";
@import "compass/css3/box-sizing";
@import "compass/css3/border-radius";

@mixin font-awesome() {
    font-family: FontAwesome;
    font-weight: normal;
    font-style: normal;
    text-decoration: inherit;
    display: inline;
    width: auto;
    height: auto;
    line-height: normal;
    vertical-align: baseline;
    background-image: none!important;
    background-position: 0 0;
    background-repeat: repeat;
    @include opacity(.7);
}



a {
    text-decoration: none !important;
}

/*@include transition-property(all);
@include transition-duration(.4s);*/
* {
    color: $font-color;
}

// this class is the stretched background. We add shadows to this since we
// can't on the body
.backstretch {
	&:after {
		content: '';
		position: fixed;
		top: 0;
		bottom: 0;
		left: 0;
		right: 0;
		z-index: 1;
	}
}

img {
	display: block;
}

//---------------------- TINYNAV START ESSENTIAL STYLES ------------------------
.tinynav { 
	display: none;
	@media only screen and (max-width: 828px) {
		display: block;
		width: 100%;
	}
	margin-bottom: 0;
}
//---------------------- TINYNAV END ESSENTIAL STYLES ------------------------


a, img,
.bfi_accordion > li > h4,
.bfi_accordion > li > h4:before {
    @include transition-property(all);
    @include transition-duration(.4s);
}



.main.container {
    width: 1020px;
    @media only screen and (min-width: 828px) and (max-width: 1020px) {
        width: 828px;
    }
    @media only screen and (max-width: 828px) {
        width: 100%;
    }
    > .content {
        margin-bottom: 30px;
        > .container.title {
            background: white;
            @include border-top-radius($radius);
            line-height: 50px;
            padding: 0 10px;
            @media only screen and (max-width: 828px) {
                @include border-radius(0);
                padding: 0;
            }
            h2 {
                margin-bottom: 0;
                padding: 5px 0 7px;
            }
        }
        > .container.body {
            @include border-radius($radius);
            margin-bottom: 20px;
            background: #efefef;
            @include box-shadow(inset 0 0 3px #fff);
            padding: 20px 10px;
            @media only screen and (max-width: 828px) {
                @include border-radius(0);
                padding: 20px 0;
                margin-bottom: 40px;
            }
            > div > p:last-child {
                margin-bottom: 0;
            }
        }
        > .container.body.with-title {
            font-size: 14px;
            line-height: 21px;
            @include border-bottom-radius($radius);
            @include border-top-radius(0);
            @media only screen and (max-width: 828px) {
                @include border-radius(0);
                padding: 20px 0;
            }
            .button {
                float: right;
                margin: 0 0 0 20px;
            }
        }
        > .container.no-color {
            background: none;
            @include box-shadow(none);
            padding: 10px 0;
        }
        > .container {
            @media only screen and (max-width: 828px) {
                width: 100%;
            }
            @media only screen and (max-width: 828px) {
                > .columns {
                    float: none;
                    display: block;
                    margin: 0 auto;
                }
            }
        }
    }
}

header.container {
    padding-top: 200px; // this should depend on what the height of the pagemedia
    @media only screen and (max-width: 828px) {
        padding-top: 0 !important;
    }
    .navbar.container {
        background: black; // navbar color
        position: absolute;
        @include border-radius($radius);
        margin: 30px 20px 0;
        padding: 0 10px;
        line-height: 50px;
        @media only screen and (max-width: 828px) {
            position: relative;
            @include border-radius(0);
            width: 100%;
            margin: 0;
            padding: 0;
            > .columns {
                float: none;
                margin: 0 auto;
                display: block;
            }
            
            padding-bottom: 20px;
            padding-top: 20px;
            height: auto;
        }
    }
    #heading-box-container {
         position: absolute;
         top: 0;
         width: 100%;
         
          z-index: 0;
        @media only screen and (max-width: 828px) {
            position: relative;
        }
        > div {
            width: 100%;
        }
        &:after {
            bottom: 0;
            width: 100%;
            height: 170px;
            content: '';
            z-index: 0;
            position: absolute;
            @media only screen and (max-width: 828px) {
                visibility: hidden;
            }
        }
        .nivoSlider {
            z-index: 0;
        }
/*        .slider-wrapper {
             position: absolute;
             top: 0;
        }*/
        .nivo-controlNav {
    		padding: 0;
			z-index: 2;
            position: absolute;
            right: 60px;
            bottom: 145px;
            @media only screen and (min-width: 828px) and (max-width: 1020px) {
                right: 40px;
                bottom: 115px;
            }
            @media only screen and (max-width: 828px) {
                bottom: 20px;
            }
    		a {
    			line-height: 0;
    			display:inline-block;
    			vertical-align: top;
    			width:38px;
    			background: rgba(0,0,0,.4);
    			text-indent:-9999px;
    			height: 10px;
    			width: 10px;
    			margin: 0 7px 0 0;
                background: white;
    			@include border-radius(5px);
    			@include opacity(.6);
                @include box-shadow(0 0 2px rgba(#000, .5));
    		}
    		a.active {
    			@include opacity(1);
                @include box-shadow(0 0 4px rgba(#000, .7));
    		}
    	}
    }
}


.widget {
    margin-bottom: 40px;
    font-size: 95%;
    
    // lists (archives, categories, custom menus, etc...)
    li, li a, li span, li abbr {
        font-size: 95%;
    }
    li {
        border-bottom: 1px solid rgba(0, 0, 0, 0.06);
        padding-bottom: 10px;
        &:last-child {
            border-bottom: 0;
            padding-bottom: 0;
        }
        // nested heirarchy lists
        ul {
            margin-top: 10px;
            margin-bottom: 0;
            border-top: 1px solid rgba(0, 0, 0, 0.06);
            padding-top: 10px;
            font-size: 100%;
            li {
                margin-bottom: 10px;
                a {
                    font-size: 100%;
                }
                &:last-child {
                    margin-bottom: 0;
                }
            }
        }
    }
    li:before {
        @include font-awesome();
        content: "\f105";
        margin-right: 5px;
    }
    > hr {
        margin-bottom: 15px;
    }
    
    // language switcher
    &.widget_bfiwidgetlanguageswitchermodel {
        .language_switcher {
            text-align: center;
        }
    }
    // portfolio widget
    &.widget_bfiwidgetportfoliomodel {
        li:before {
            content: '';
        }
        .lightbox, .fancybox {
            width: 100%;
            height: auto;
            font-size: 100%;
            margin-bottom: 0;
            &:before {
                font-size: 3em;
            }
            img {
                width: 100%;
            }
        }
        div {
            margin-top: 5px;
        }
    }
    // calendar widget
    &.widget_calendar {
        #wp-calendar {
            width: 100%;
            text-align: center;
            margin-bottom: 20px;
            caption {
                font-weight: bold;
                margin-bottom: 5px;
                &:before {
                    @include font-awesome();
                    content: "\f073";
                    margin-right: 5px;
                }
            }
            td, th {
                line-height: 30px;
            }
            th {
                background: rgba(0,0,0,.5); //#999;
                color: white;
            }
            tr {
                border-bottom: 1px solid #eee;
            }
            tbody {
                tr:nth-child(even) {
                    background: white;
                }
                background: rgba(255,255,255,.8);//#fff;
                tr:last-child {
                    border: 0;
                }
            }
            tfoot {
                a {
                    margin-top: 5px;
                }
                tr {
                    border: 0;
                }
            }
            td a {
                display: block;
                text-decoration: none;
                font-weight: bold;
                line-height: 31px;
                &:hover {
                    background: #666;
                    color: white;
                }
            }
        }
    }
    // twitter
    &.widget_bfiwidgettwittermodel {
        .bfi-twitter-widget {
            padding-left: 30px;
            li {
                position: relative;
                &:before {
                    content: "\f099";
                    font-size: 150%;
                    left: -30px;
                    position: absolute;
                }
            }
        }
        .twitter-follow-button {
            margin-bottom: 10px;
        }
    }
    // recent comments
    &.widget_recent_comments {
        li:before {
            content: "\f075";
        }
    }
    // RSS feed
    &.widget_rss {
        > h4 {
            > .cufon {
                display: none !important;
            }
            .rsswidget {
                &:first-child {
                    display: none;
                }
            }
        }
        .rssSummary {
            margin-top: 10px;
        }
        cite {
            margin-top: 5px;
            display: block;
            font-size: 90%;
            font-style: italic;
        }
        li:before {
            content: "\f09e";
        }
    }
    // archive
    &.widget_archive {
        li:before {
           content: "\f073"; 
        }
    }
    // fix for recent entries
    &.widget_recent_entries {
        li:before {
            margin-right: 0;
        }
    }
    // categories
    &.widget_categories {
        li:before {
            content: "\f07c";
        }
    }
    // recent blog posts
    &.widget_recent_entries {
        li:before {
            content: "\f040";
        }
    }
    // pages list
    &.widget_pages {
        li:before {
            content: "\f0f6";
        }
    }
    &.widget_bfiwidgetflickrmodel {
        a:hover img {
            filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=70) !important;
            opacity: 0.7 !important;
        }
    }
}


