.bfi_accordion {
	li {
		position: relative;
		overflow: hidden;
		> h4 {
		    @include border-radius($radius);
			background: rgba(0, 0, 0, .06);
			padding: 8px 10px;
			cursor: pointer;
			&:after {
				content: '\f067';
				font-family: FontAwesome;
				font-size: 14px;
				float: left;
                margin-right: 10px;
                padding-top: 2px;
			}
		}
		> h4.selected {
			&:after {
				content: '\f068';
				font-family: FontAwesome;
				font-size: 14px;
				float: left;
                margin-right: 10px;
                padding-top: 2px;
			}
		}
		> div {
			padding: 15px 20px;
			display: none;
			> p:last-child {
			    margin-bottom: 0;
			}
		}
	}
