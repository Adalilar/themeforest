// info boxes
.bfi_infobox {
	padding: 10px 20px;
	margin-bottom: 20px;
	cursor: pointer;
	background: rgba(0,0,0,.06);
	i:first-child {
		float: left;
		margin: 0 20px 0 0;
	}
	i:nth-child(2) {
		float: right;
		margin: 4px 0 0 20px;
	}
	> div {
		overflow: hidden;
		margin-top: 4px;
	}
	
	// different types of info boxes
	&.notice {
		background: #F1F2DC;
		color: #B55F31;
		border: 1px solid #E3E6B5;
		> div, i, strong, em {
			color: #B55F31;
		}
	}
	&.success {
		background: #DCF2DD;
		color: #509C41;
		border: 1px solid #BBE6B5;
		> div, i, strong, em {
			color: #509C41;
		}
	}
	&.info {
		background: #DCEFF2;
		color: #347794;
		border: 1px solid #B5E5E6;
		> div, i, strong, em {
			color: #347794;
		}
	}
	&.error {
		background: #F2DFDC;
		color: #9C3636;
		border: 1px solid #E6B5B5;
		> div, i, strong, em {
			color: #9C3636;
		}
	}
}

